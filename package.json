{
  "name": "vgbnd",
  "version": "1.0.0",
  "private": true,
  "description": "VGBND - AI Travel Planner Desktop App",
  "main": "electron/main.js",
  "scripts": {
    "start-vite": "vite",
    "start-electron": "wait-on http://localhost:5173 && electron electron/main.js",
    "dev": "concurrently \"npm run start-vite\" \"npm run start-electron\"",
    "build": "vite build",
    "package": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.vgbnd.app",
    "productName": "VGBND Travel AI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**",
      "electron/**",
      "backend/**",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    }
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "animate.css": "^4.1.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "bootstrap": "^5.3.6",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "framer-motion": "^12.16.0",
    "groq-sdk": "^0.37.0",
    "html2pdf.js": "^0.10.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.2",
    "openai": "^6.9.1",
    "react": "^19.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "react-scroll": "^1.9.3",
    "react-slick": "^0.30.3",
    "react-toastify": "^11.0.5",
    "slick-carousel": "^1.8.1",
    "typed.js": "^2.1.0",
    "vercel": "^25.2.0",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^8.2.2",
    "electron": "^37.2.1",
    "electron-builder": "^24.13.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.5",
    "wait-on": "^7.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omkarr10/vgbnd.git"
  },
  "bugs": {
    "url": "https://github.com/omkarr10/vgbnd/issues"
  },
  "homepage": "https://github.com/omkarr10/vgbnd#readme",
  "license": "ISC"
}
